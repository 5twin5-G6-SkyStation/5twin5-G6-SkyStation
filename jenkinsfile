pipeline {
    agent any

    stages {
        stage('Getting project from Git') {
            steps {
                script {
                    checkout([$class: 'GitSCM', branches: [[name: 'widedgharbi-5twin5-G6']], userRemoteConfigs: [[url: 'https://github.com/5twin5-G6-SkyStation/5twin5-G6-SkyStation.git']]])
                }
            }
        }

        stage('Maven Clean & Compile') {
            steps {
                sh 'mvn clean compile -B -DskipTests'
            }
        }

       
stage('SonarQube Analysis') {
            steps{
             		sh "  mvn clean verify sonar:sonar -Dsonar.projectKey=cicd -Dsonar.projectName='cicd' -Dsonar.host.url=http://172.17.0.1:9000 -Dsonar.token=sqp_2d9c32825b84fd25555d9230e8b94561bfb89119 "

            }
        }
}
}
